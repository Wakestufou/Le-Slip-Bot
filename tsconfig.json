{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "moduleDetection": "force",
      "baseUrl": "./src",
      "paths": {
        "@utils/*": ["utils/*"]
      },
      "outDir": "dist",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true
    },
    "include": ["src"]
  }
  